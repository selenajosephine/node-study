import org.junit.Test;
import org.junit.runner.RunWith;
import org.springframework.test.context.junit4.SpringRunner;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;
import java.util.List;

import static org.junit.Assert.assertEquals;
import static org.junit.Assert.assertNotNull;

@RunWith(SpringRunner.class)
public class AgentRiskFlagConfigTest {

    @Test
    public void testLowRiskFlagActionList() {
        AgentRiskFlagConfig agentRiskFlagConfig = new AgentRiskFlagConfig();

        // Test default value
        assertNotNull(agentRiskFlagConfig.getLowRiskFlagActionList());

        // Test setter and getter
        List<String> lowRiskActions = Arrays.asList("Action1", "Action2");
        agentRiskFlagConfig.setLowRiskFlagActionList(lowRiskActions);
        assertEquals(lowRiskActions, agentRiskFlagConfig.getLowRiskFlagActionList());
    }

    @Test
    public void testHighRiskFlagActionList() {
        AgentRiskFlagConfig agentRiskFlagConfig = new AgentRiskFlagConfig();

        // Test default value
        assertNotNull(agentRiskFlagConfig.getHighRiskFlagActionList());

        // Test setter and getter
        List<String> highRiskActions = Arrays.asList("Action3", "Action4");
        agentRiskFlagConfig.setHighRiskFlagActionList(highRiskActions);
        assertEquals(highRiskActions, agentRiskFlagConfig.getHighRiskFlagActionList());
    }

    @Test
    public void testNullLowRiskFlagActionList() {
        AgentRiskFlagConfig agentRiskFlagConfig = new AgentRiskFlagConfig();

        // Test null lowRiskFlagActionList
        agentRiskFlagConfig.setLowRiskFlagActionList(null);
        assertNotNull(agentRiskFlagConfig.getLowRiskFlagActionList());
    }

    @Test
    public void testNullHighRiskFlagActionList() {
        AgentRiskFlagConfig agentRiskFlagConfig = new AgentRiskFlagConfig();

        // Test null highRiskFlagActionList
        agentRiskFlagConfig.setHighRiskFlagActionList(null);
        assertNotNull(agentRiskFlagConfig.getHighRiskFlagActionList());
    }

    @Test
    public void testBothNullLists() {
        AgentRiskFlagConfig agentRiskFlagConfig = new AgentRiskFlagConfig();

        // Test null lowRiskFlagActionList and highRiskFlagActionList
        agentRiskFlagConfig.setLowRiskFlagActionList(null);
        agentRiskFlagConfig.setHighRiskFlagActionList(null);

        assertNotNull(agentRiskFlagConfig.getLowRiskFlagActionList());
        assertNotNull(agentRiskFlagConfig.getHighRiskFlagActionList());
    }
}
